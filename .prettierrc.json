{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 100,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "es5",
  "plugins": ["prettier-plugin-go-template"],
  "overrides": [
    {
      "files": "themes/karuta/layouts/**/*.html",
      "options": {
        "parser": "go-template"
      }
    }
  ]
}
